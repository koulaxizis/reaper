 while ( count_ < num ) 
 ( 
 x = incWidth * count_ -radius; 
 y = sin( $pi *2 * time_ + incDelta*count_ ) * gfx_h/2.3 + gfx_h/2; 
 gfx_circle(x,y,radius,true); 
 x = incWidth * count_ -radius; 
 y = sin( $pi *2 * time_ + incDelta*count_ +$pi )* gfx_h/2.3 + gfx_h/2; 
 gfx_circle(x,y,radius,true); 
 count_+=1; 
 ); 
