function drawSinWaveCircle(num,radius,rate,speed) 
 local (count_,incWidth,incDelta,x,y,time_) 
( 
 incWidth = gfx_w / num; 
 incDelta = ($pi *2 *rate)/num; 
 gfx_r=1; 
 gfx_g=1; 
 gfx_b=1; 
 gfx_a=1; 
