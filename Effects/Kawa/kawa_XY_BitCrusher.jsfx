); 
function getBezierPointQB_Y(time_,x1,y1,bX,bY,x2,y2) local (tp,yout) 
( 
 tp = 1 - time_; 
 yout = time_*time_*y2 + 2*time_*tp*bY + tp*tp*y1; 
