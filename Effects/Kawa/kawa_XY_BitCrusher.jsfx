 fftAnalyzeFFTArray = FREQUENCY_getFftAnalizeLinePointArray(FREQUENCY_ID); 
 ARRAY_clear( fftAnalyzeFFTArray); 
 count_RR = 0; 
 while(count_RR < fftSize/2) 
 ( 
 ARRAY_push_back(fftAnalyzeFFTArray,POINT_create(0,0)); 
 count_RR +=1; 
 ); 
