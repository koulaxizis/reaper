 local ( btGroup_BPM 
 , btGroup_MUL 
 , delaySample 
 , delayGainValue 
 , feedBackValue 
 , panValue 
 , delayArray 
 , effectedValue 
 , panner_class 
 , wetDryValue 
 ) 
