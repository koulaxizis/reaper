 ly = y_ -oy; 
 px = lx * cos(AngleRadian_) - ly *sin(AngleRadian_); 
 py = lx * sin(AngleRadian_) + ly *cos(AngleRadian_); 
 this.x = ox +px; 
 this.y = oy +py; 
); 
