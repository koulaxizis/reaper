function FREQUENCY_YtoDB(FREQUENCY_ID,y_ ) local (out,cY,maxDb,halfHeight) 
( 
 cY = RECTANGLE_getCenterY(FREQUENCY_ID); 
 halfHeight = RECTANGLE_getHeight(FREQUENCY_ID)/2; 
