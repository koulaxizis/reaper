); 
function HSBtoRGB(hue, saturation , value) 
 local ( r,g,b,h,f,v,p,q,t) 
( 
 ( hue > 360) ? ( hue = hue % 360;) 
 :( hue < 0 ) ? ( hue = hue % 360 ;); 
 saturation = min ( max(saturation,0),1); 
 v = floor( 255 * value); 
 v = min ( max(v,0),255); 
 (saturation == 0)? 
 ( 
 r = v ; g = v; b = v; 
 ):( 
 h = floor( hue / 60); 
 f = hue / 60-h; 
 p = floor( v * (1-saturation)); 
 p = min ( max(p,0),255); 
 q = floor( v * (1-f * saturation)); 
 q = min ( max(q,0),255); 
 t = floor( v * (1-(1-f) * saturation)); 
 t = min ( max(t,0),255); 
 (h==0)?(r=v;g=t;b=p;) 
 :(h==1)?(r=q;g=v;b=p;) 
 :(h==2)?(r=p;g=v;b=t;) 
 :(h==3)?(r=p;g=q;b=v;) 
 :(h==4)?(r=t;g=p;b=v;) 
 :(h==5)?(r=v;g=p;b=q;) 
 :( r=v;g=t;b=p;); 
 ); 
 this.r = r; 
 this.g = g; 
 this.b = b; 
); 
