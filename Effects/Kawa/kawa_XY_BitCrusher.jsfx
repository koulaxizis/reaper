); 
function getBezierAngleCB(t,x1,y1,bX1,bY1,bX2,bY2,x2,y2) local (tp,dx,dy,angleRad) 
( 
 tp = 1 - t; 
 dx = 3*(t*t*(x2-bX2)+2*t*tp*(bX2-bX1)+tp*tp*(bX1-x1)); 
 dy = 3*(t*t*(y2-bY2)+2*t*tp*(bY2-bY1)+tp*tp*(bY1-y1)); 
